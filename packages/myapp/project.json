{
  "name": "myapp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/myapp/src",
  "targets": {
    "deploy": {
      "executor": "@stellarlibs/nx-cdk:deploy",
      "options": {}
    },
    "destroy": {
      "executor": "@stellarlibs/nx-cdk:destroy",
      "options": {}
    },
    "bootstrap": {
      "executor": "@stellarlibs/nx-cdk:bootstrap",
      "options": {}
    },
    "synth": {
      "executor": "@stellarlibs/nx-cdk:synth",
      "options": {}
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/myapp/jest.config.ts"
      }
    },
    "list-stacks": {
      "executor": "@stellarlibs/nx-cdk:list-stacks",
      "options": {}
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/myapp/**/*.ts"]
      }
    }
  },
  "tags": []
}
